  <ItemGroup>
    <PackageReference Include="Microsoft.Office.Interop.Excel" Version="15.0.4795.1000" />
  </ItemGroup>
	

  <ItemGroup>
    <ProjectReference Include="..\Otus.Lib4Preprocessor\Otus.Lib4Preprocessor.csproj" />
  </ItemGroup>

</Project>
